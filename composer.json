    "keywords": ["1c-integration", "1c-symfony", "1c",  "1c-php", "1s-integration", "1s-symfony", "1s", "CommerceML"],
    "homepage": "https://github.com/PavloPashchevskyi",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Kartsev",
            "role": "Developer"
        },
        {
            "name": "Pavlo Pashchevskyi",
            "email": "googalltooth@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Iks\\Symfony1SIntegration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "carono/commerceml": "^0.2.5"
